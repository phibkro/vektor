{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@ui/*": ["src/components/ui/*"],
			"@lib/*": ["src/lib/*"],
			"@images/*": ["src/images/*"]
		}
	}
}
